{
  "name": "spworlds.ru",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "private": true,
  "scripts": {
    "build": "tsc && npm run pub-build && npm run admin-build",
    "dev": "tsc -w",
    "start": "node ./dist/index.js",
    "pub-dev": "cd public && concurrently --kill-others \"sass --watch --no-source-map src/:./dist/\" \"tsc -w\"",
    "pub-build": "cd public && tsc && sass --no-source-map src/:./dist/ && autoprefixer-cli -o ./dist/styles.css ./dist/styles.css",
    "admin-build": "cd admin && npm run build"
  },
  "dependencies": {
    "@prisma/client": "^2.19.0",
    "argon2": "^0.27.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "fastify": "^3.14.0",
    "fastify-formbody": "^5.0.0",
    "fastify-jwt": "^2.4.0",
    "fastify-static": "^4.0.1",
    "form-data": "^4.0.0",
    "node-fetch": "^2.6.1",
    "npm": "^7.7.5",
    "path": "^0.12.7",
    "point-of-view": "^4.14.0"
  },
  "devDependencies": {
    "@types/form-data": "^2.5.0",
    "@types/node": "^14.14.35",
    "autoprefixer-cli": "^1.0.0",
    "concurrently": "^6.0.0",
    "prisma": "^2.19.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
