datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

generator client {
  provider = "prisma-client-js"
}

model Server {
  id             Int    @id @default(autoincrement())
  name           String @unique
  description    String
  price          Int
  donationalerts String
  image          String
}

model User {
  username String  @id
  password String
  isAdmin  Boolean
}
